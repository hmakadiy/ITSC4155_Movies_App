<%- include('../partials/header.ejs')%>
<form action = '/users/login' method = 'POST'>
    <div class="justify-content-center">
        <div class="form-group">
            <label for="email" class="form-label">Email: </label>
            <input type="email" class="form-control" id="email" name="email" required></td>
        </div>       
        <div class="form-group">
            <label for="password" class="form-label">Password: </label>
            <input type="password" class="form-control" id="password" name="password" minlength="8" maxlength="64" required></td>
        </div><br>
            <button style="text-align: center;" type="submit" class="btn btn-primary">Login</button>
    </div>     
</form>

</div>
